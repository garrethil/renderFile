<div class="row">
    <div id="sidebar" class="col-md-3 py-5">
        <div class="sidebar-header my-3 font-normal">
            <h3>Freshest Rendering</h3>
        </div>
        <div class="sidebar-content">
            <div class="iframe-container">
                <iframe src="https://www.youtube.com/embed/iBNHARrP30I?si=jmbrGsuXQdhLIcoi" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
            </div>        
        </div>
    </div>

    <div class="main-text col-md-9 justify-content-center py-5 font-normal">
        <h2>Up Coming</h2>
<table class="table table-striped">
            <thead>
                <tr>
                    <th class="underline">Game</th>
                    <th class="underline">Location</th>
                    <th class="underline">Date</th>
                </tr>
            </thead>
            <tbody>
                <!-- Add your table rows here -->
                {{#each eventData as |event|}}
                <tr>
                    <td>{{event.game}}</td>
                    <td>{{event.location}}</td>
                    <td>{{event.date}}</td>
                </tr>
                {{/each}}
                <!-- Add more rows as needed -->
            </tbody>
        </table>
    </div>
</div>
